{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:vue/vue3-recommended", "eslint:recommended", "@vue/typescript/recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "plugins": ["vue", "@typescript-eslint"],
  "rules": {
    "vue/padding-line-between-blocks": ["error", "always"],
    "vue/multi-word-component-names": ["off"]
  },
  "ignorePatterns": ["dist/"]
}
